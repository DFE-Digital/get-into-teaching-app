  <%= meta_tag(key: 'type', value: 'website', opengraph: true) %>
  <%= meta_tag(key: 'title', value: page_title(@page_title, @front_matter), opengraph: true) %>
  <%= meta_tag(key: 'site_name', value: "Get Into Teaching", opengraph: true) %>
