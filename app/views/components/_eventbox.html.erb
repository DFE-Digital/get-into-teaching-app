<div class="event-resultbox">
    <div class="event-resultbox__header">
        <h2><%= title %></h2>
    </div>
    <div class="event-resultbox__datetime">
        <%= datetime %>
    </div>
    <div class="event-resultbox__greenstripe"></div>
    <div class="event-resultbox__content">
        <p>
            <%= description %>
        </p>
        <div><h3><div class='event-resultbox__content__icon icon-train-to-teach'></div> Train to teach event</h3></div>
        <div><h3><div class='event-resultbox__content__icon icon-online'></div> Online event</h3></div>
        <div><h3><div class='event-resultbox__content__icon icon-pin'></div> <%= location %></h3></div>
    </div>
</div>