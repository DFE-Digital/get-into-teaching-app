<section class="event-reg-main">
    <div class="event-reg-main__content container-1000">
        <div class="event-reg-main__content__left">
            <h1>Sign up for this event:</h1>
            <h2>Train to Teach Coventry online event</h2>
            <p></p>

            <%= govuk_form_for current_step, url: step_path do |f| %>
              <%= f.govuk_error_summary %>

              <%= f.govuk_text_field :phone_number %>

              <%= f.button class: "call-to-action-button" do %>
                Next Step <%= fas "chevron-right" %>
              <% end %>
            <% end %>
        </div>
        <div class="event-reg-main__content__right">

        </div>
    </div>
</section>

<%= render "sections/talk-to-us" %>
