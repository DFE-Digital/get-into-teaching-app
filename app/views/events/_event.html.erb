<%= link_to event_path(event.id) do %>
    <%= render "components/eventbox",
        :title => event.name,
        :datetime => format_event_date(event),
        :description => event.description,
        :schooloruni => false,
        :traintoteach => true,
        :online => true,
        :location => "Bristol"
    %>
<% end %>
