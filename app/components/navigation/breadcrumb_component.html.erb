<section class="container container--narrow">
  <nav class="breadcrumb<%= " breadcrumb--hero" if @under_hero %>" aria-label="breadcrumb">
    <ol>
      <% helpers.breadcrumb_trail do |crumb| %>
        <% next if crumb.current? %>
        <li><%= link_to crumb.name, crumb.url %></li>
      <% end %>
    </ol>
  </nav>
</section>
