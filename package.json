  "jest": {
    "testRegex": ".*_spec.js",
    "roots": [
      "spec/javascript"
    ],
    "testEnvironment": "jsdom",
    "moduleDirectories": [
      "node_modules",
      "app/webpacker/controllers",
      "app/webpacker/javascript",
      "app/webpacker/packs"
